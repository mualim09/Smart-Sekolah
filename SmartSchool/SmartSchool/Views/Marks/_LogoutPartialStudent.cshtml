@using Microsoft.AspNet.Identity
@if (Request.Cookies.Get("student") != null)
{
    using (Html.BeginForm("Logout", "Students", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
        @Html.AntiForgeryToken()

        <ul class="nav navbar-nav navbar-right">
            
            <li><a href="javascript:document.getElementById('logoutForm').submit()">@ViewBag.User Logout</a></li>
        </ul>
    }
}
